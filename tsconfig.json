{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "assets/jss/nextjs-material-kit/pages/landingPageSections/workStyle.js",
    "assets/jss/nextjs-material-kit/pages/landingPageSections/productStyle.js",
    "assets/jss/nextjs-material-kit/pages/landingPageSections/packageStyle.js",
    "src/components/Header/Header.js", "assets/jss/nextjs-material-kit/pages/landingPage.js",
    "assets/jss/nextjs-material-kit.js",
    "pages-sections/LandingPage-Sections/ProductSection.js"  ],
  "exclude": [
    "node_modules"
  ]
}
